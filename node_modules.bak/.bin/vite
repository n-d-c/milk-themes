#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/var/www/devlove-milk/node_modules/.pnpm/vite@2.3.3/node_modules/vite/bin/node_modules:/var/www/devlove-milk/node_modules/.pnpm/vite@2.3.3/node_modules/vite/node_modules:/var/www/devlove-milk/node_modules/.pnpm/vite@2.3.3/node_modules:/var/www/devlove-milk/node_modules/.pnpm/node_modules:/var/www/devlove-milk/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/devlove-milk/node_modules/vite/bin/node_modules:/var/www/devlove-milk/node_modules/vite/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
