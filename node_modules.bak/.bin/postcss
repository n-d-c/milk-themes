#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/var/www/devlove-milk/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.2.8/node_modules/postcss-cli/bin/node_modules:/var/www/devlove-milk/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.2.8/node_modules/postcss-cli/node_modules:/var/www/devlove-milk/node_modules/.pnpm/postcss-cli@8.3.1_postcss@8.2.8/node_modules:/var/www/devlove-milk/node_modules/.pnpm/node_modules:/var/www/devlove-milk/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/devlove-milk/node_modules/postcss-cli/bin/node_modules:/var/www/devlove-milk/node_modules/postcss-cli/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../postcss-cli/bin/postcss" "$@"
else
  exec node  "$basedir/../postcss-cli/bin/postcss" "$@"
fi
