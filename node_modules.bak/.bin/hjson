#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/var/www/devlove-milk/node_modules/.pnpm/hjson@3.2.2/node_modules/hjson/bin/node_modules:/var/www/devlove-milk/node_modules/.pnpm/hjson@3.2.2/node_modules/hjson/node_modules:/var/www/devlove-milk/node_modules/.pnpm/hjson@3.2.2/node_modules:/var/www/devlove-milk/node_modules/.pnpm/node_modules:/var/www/devlove-milk/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/devlove-milk/node_modules/hjson/bin/node_modules:/var/www/devlove-milk/node_modules/hjson/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hjson/bin/hjson" "$@"
else
  exec node  "$basedir/../hjson/bin/hjson" "$@"
fi
